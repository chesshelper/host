    "next": "15.1.7",
    "next-i18next": "^15.4.2",
    "next-intl": "^3.26.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "wrangler": "^3.112.0"
